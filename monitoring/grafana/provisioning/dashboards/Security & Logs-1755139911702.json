          "editorMode": "code",
          "expr": "sum by (error_category) (\r\n  count_over_time(\r\n    {job=\"docker-containers\"} \r\n    |~ \"api-prod\" \r\n    | regexp \"(?P<error_category>(?i)error|failed|exception|warn|statusCode\\\":4[0-9]{2}|statusCode\\\":5[0-9]{2})\" \r\n    [5m]\r\n  )\r\n)",
          "queryType": "range",
          "refId": "Err"
        }
      ],
      "title": "Error Log Types",
      "transformations": [
        {
          "id": "renameByRegex",
          "options": {
            "regex": ".*error_category=\"(error|ERROR)\".*",
            "renamePattern": "Error"
          }
        },
        {
          "id": "renameByRegex",
          "options": {
            "regex": ".*error_category=\"warn\".*",
            "renamePattern": "Warning"
          }
        },
        {
          "id": "renameByRegex",
          "options": {
            "regex": ".*\\{\\}.*",
            "renamePattern": "Other"
          }
        },
